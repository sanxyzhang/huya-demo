.LeaderBoardMain {
  display: flex;
  flex: 1;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  position: relative;
  min-height: 270;
}

.boardItem {
  position: absolute;
  height: 44;
  margin-bottom: 10px;
  width: 300;
  transitionProperty: top;
  transitionDuration: 1000ms;
}

.boardContent {
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items: center;
  height: 100%;
}

.roomText {
  font-size: 18;
  text-align: center;
  color: white
}

.textLine {
  height: 2;
  width: 300;
  margin: 0 auto;
  margin-bottom: 8;

}
.btnGroup{
  width: 375;
  display: flex;
  justify-content: space-evenly;
  flex-direction: row;
  align-items: center;
}

.imgLogo {
  margin-left: 20;
  color: white;
}

.userName {
  color: white;
  font-size: 18;
}

.itemNum {
  margin-right: 20;
  color: white;
  font-size: 18;
}

.gameRuleContent,
.gameRuleTitle {
  text-align: center;
  color: white;
  font-size: 18;
}

.startBtnBg{
  height: 48;
  width: 142;
}

.white_24{
  color: white;
  font-size: 24;
}
.guideView{
  display:flex;
  flex-direction: row;
}
.white_20{
  color: white;
  font-size: 20;
}
.guide{
  width: 16;
  height: 16;
  margin-top: 5;
  margin-left: 5;
}
.startBtn{
  background: transparent;
  border: 0;
  width: 140;
  height: 48;
}

.leaveBtnBg {
  height: 48;
  width: 142;
}

.leaveBtn {
  background: transparent;
  border: 0;
  width: 140;
  height: 48;
}

.modalLeaveBtnBg{
  height: 48;
  width: 142;
  margin-left: 20;
}

.modalLeaveBtnOnlyBg{
  height: 48;
  width: 142;
  margin-left: 20;
}

.modalCancelBtnBg {
  height: 48;
  width: 142;
}

.modalCancelBtn,
.modalLeaveBtn{
  background: transparent;
  border: 0;
  width: 140;
  height: 48;
}

.confirmLeaveBg {
  width: 320;
  height: 168;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.modalBtnGroup{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.onlyBtn{
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-right: 8;
}
.wihteCommonText {
  color: white;
  text-align: center;
  font-size: 18;
  height: 100%;
}
.scrollView{
  height: 110;
  overflow: auto;
  margin-top: 5;
}

.groupImg{
  width: 32;
  height: 32;
  border-radius: 50%;
  align-self: center;
  margin-left:10;
}
